<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated cdmModel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.rra.cdmModel.presentation.CDMModelModelWizardID"
            name="%_UI_CDMModelModelWizard_label"
            class="org.rra.cdmModel.presentation.CDMModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CDMModelModelFile.gif">
         <description>%_UI_CDMModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated cdmModel -->
      <editor
            id="org.rra.cdmModel.presentation.CDMModelEditorID"
            name="%_UI_CDMModelEditor_label"
            icon="icons/full/obj16/CDMModelModelFile.gif"
            extensions="cdmmodel"
            class="org.rra.cdmModel.presentation.CDMModelEditor"
            contributorClass="org.rra.cdmModel.presentation.CDMModelActionBarContributor">
      </editor>
   </extension>

</plugin>
