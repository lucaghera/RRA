<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated runtimeFeatureModel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.rra.runtimeFeatureModel.presentation.RuntimeFeatureModelModelWizardID"
            name="%_UI_RuntimeFeatureModelModelWizard_label"
            class="org.rra.runtimeFeatureModel.presentation.RuntimeFeatureModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RuntimeFeatureModelModelFile.gif">
         <description>%_UI_RuntimeFeatureModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated runtimeFeatureModel -->
      <editor
            id="org.rra.runtimeFeatureModel.presentation.RuntimeFeatureModelEditorID"
            name="%_UI_RuntimeFeatureModelEditor_label"
            icon="icons/full/obj16/RuntimeFeatureModelModelFile.gif"
            extensions="runtimefeaturemodel"
            class="org.rra.runtimeFeatureModel.presentation.RuntimeFeatureModelEditor"
            contributorClass="org.rra.runtimeFeatureModel.presentation.RuntimeFeatureModelActionBarContributor">
      </editor>
   </extension>

</plugin>
